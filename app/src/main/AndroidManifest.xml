<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cs4310.epsilon.buynutsproto" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".activities.MainLoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.NewsActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/activity_title_news" />
        <activity
            android:name=".activities.MakeOfferActivity"
            android:label="@string/activity_title_make_offer" />
        <activity
            android:name=".activities.SetSearchFilterActivity"
            android:label="@string/activity_title_set_search_filter" />
        <activity
            android:name=".activities.ViewSellOfferActivity"
            android:label="@string/activity_title_view_sell_offer" />
        <activity
            android:name=".activities.ContactSellerActivity"
            android:label="@string/activity_title_contact_seller" />
        <activity android:name=".activities.RegistrationActivity" >
        </activity>
    </application>

</manifest>
